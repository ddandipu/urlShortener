<header style = "background-color : pink">
   <h1> TinyApp</h1>
   <% if (userid === undefined) { %>
   <a href="/register">Please register</a>
   <a href="/login">Or Login</a>
   <% } else { %>
   <p>
     <%=  userid %><br>
     <%= usermail %><br>
     <%= userpass %><br>
     <form action="/logout" method="POST">
     <input type="submit" value="Logout">
     </form>
   </p>
   <% } %>
</header>